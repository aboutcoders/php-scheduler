  "keywords": [
    "cron",
    "schedule",
    "scheduler"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Schulz",
      "email": "hannes.schulz@aboutcoders.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "psr/log": "^1.1",
    "dragonmantank/cron-expression": "^2.3",
    "myclabs/php-enum": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "symfony/console": "^4.3|5.0"
  },
  "autoload": {
    "psr-4": {
      "Abc\\Scheduler\\": "src/"
